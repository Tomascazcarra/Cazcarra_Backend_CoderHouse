<div>
    <h1>Productos registrados en MONGO</h1>
    {{#each products}}
    <div>
        <p> Titulo:
            {{this.title}}
        </p>
        <p> Descripcion:
            {{this.description}}
        </p>
        <p> Precio:
            {{this.price}}
        </p>
        <p> Stock disponible:
            {{this.stock}}
        </p>
    </div>
    {{/each}}
</div>